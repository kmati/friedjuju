{
  "name": "friedjuju",
  "version": "0.0.3",
  "repository": "https://github.com/kmati/friedjuju",
  "license": "MIT",
  "description": "A project that contains 3 libraries: (a) j2m -> JSON to markup transformer, (b) j2j -> JSON to JSON transformer and query tool, as well as (c) j2f -> bound function invocation dispatcher when traversal occurs on object graph",
  "homepage": "https://github.com/kmati/friedjuju",
  "author": {
    "name": "Kimanzi Mati",
    "email": "kmati@nocrunchtime.com",
    "url": "http://www.nocrunchtime.com"
  },
  "dependencies": {
    "uglify-js": "*",
    "browserify": "*"
  },
  "devDependencies": {
    "expresso": "*"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "build-all": "node build/build-all.js -target all",
    "build-j2m": "node build/build-j2m.js -target all",
    "build-j2j": "node build/build-j2j.js -target all",
    "build-j2f": "node build/build-j2f.js -target all",
    "test": "expresso test/*/*"
  }
}